<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="activetech.zyyhospital.dao.mapper.HspJjbhzmxCustomMapper">
  <resultMap id="BaseResultMap" type="activetech.zyyhospital.pojo.dto.HspJjbhzmxCustom">
    <id column="JJBGL_SEQ" property="jjbglSeq" jdbcType="VARCHAR" />
    <id column="EMG_SEQ" property="emgSeq" jdbcType="VARCHAR" />
    <result column="EMG_TYPE" property="emgType" jdbcType="VARCHAR" />
    <result column="MEMO" property="memo" jdbcType="VARCHAR" />
  </resultMap>
  
  <sql id="Query_Hspjjbhzmx_where">
		<if test="hspJjbhzmxCustom!=null">
			<if test="hspJjbhzmxCustom.emgSeq!=null and hspJjbhzmxCustom.emgSeq!=''">
				and EMG_SEQ = #{hspJjbhzmxCustom.emgSeq}
			</if>
			<if test="hspJjbhzmxCustom.jjbglSeq!=null and hspJjbhzmxCustom.jjbglSeq!=''">
				and JJBGL_SEQ = #{hspJjbhzmxCustom.jjbglSeq}
			</if>
		</if>
	</sql>
	
	<select id="isexit" parameterType="activetech.zyyhospital.pojo.dto.HspJjbhzmxCustom" resultType="int">
		select count(1) from hsp_jjbhzmx 
		where EMG_SEQ = #{emgSeq} and JJBGL_SEQ = #{jjbglSeq}
	</select>
	
  </mapper>